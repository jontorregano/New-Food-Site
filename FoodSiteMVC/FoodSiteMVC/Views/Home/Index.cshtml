<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <!-- Web Fonts-->
    <link href="https://fonts.googleapis.com/css?family=Poppins:500,600,700" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Hind:400,700" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Lora:400i" rel="stylesheet">
    <!-- Bootstrap core CSS-->
    <link href="~/Content/bootstrap.min.css" rel="stylesheet">
    <!-- Icon Fonts-->
    <link href="~/Content/font-awesome.min.css" rel="stylesheet">
    <link href="~/Content/linea-arrows.css" rel="stylesheet">
    <link href="~/Content/linea-icons.css" rel="stylesheet">
    <!-- Plugins-->
    <link rel="stylesheet" href="~/Content/owl.carousel.css" type="text/css">
    <link rel="stylesheet" href="~/Content/magnific-popup.css" type="text/css">
    <link rel="stylesheet" href="~/Content/vertical.min.css" type="text/css">
    <link rel="stylesheet" href="~/Content/pace-theme-minimal.css" type="text/css" />
    <link rel="stylesheet" href="~/Content/animate.css" type="text/css" />

    <!-- Template core CSS-->
    <link rel="stylesheet" href="~/Content/template.css" type="text/css" />
</head>
<body>

			<!-- Header-->
<header class="header header-center">
    <div class="container-fluid">
        <!-- Logos-->
        <div class="inner-header"><a class="inner-brand" href="index.html"><img class="brand-dark" src="assets/images/logo.png" width="77px" alt=""><img class="brand-light" src="assets/images/logo-light.png" width="77px" alt=""></a></div>
        <!-- Navigation-->
        <div class="inner-navigation collapse">
            <div class="inner-navigation-inline">
                <div class="inner-nav">
                    <ul>
                        <!-- Home-->
                        <li class="menu-item-has-children menu-item-has-mega-menu">
                            <a href="index.html">Home</a>
                        </li>
                        <!-- Home end-->
                        <!-- Pages-->
                        <li class="menu-item-has-children menu-item-has-mega-menu">
                            <a href="index.html">Contact Us</a>
                            <div class="mega-menu">
                                <ul class="sub-menu mega-menu-row">
                            </div>
                        </li>
                        <!-- Pages end-->

                        <!-- Mega menu-->
                        <li class="menu-item-has-children menu-item-has-mega-menu">
                            <a href="#">Menu</a>
                            <div class="mega-menu">
                                <ul class="sub-menu mega-menu-row">
                                    <!-- Column 1-->
                                    <li class="menu-item-has-children mega-menu-col">
                                        <a href="index.html">Breakfast</a>
                                        <ul class="sub-menu">
                                            <li><a href="index.html"></i>Sandwiches</a></li>
                                            <li><a href="index.html"></i>Plates</a></li>
                                        </ul>
                                    </li>
                                    <!-- Column 2-->
                                    <li class="menu-item-has-children mega-menu-col">
                                        <a href="index.html">Poboys</a>
                                        <ul class="sub-menu">
                                            <li><a href="index.html"></i>Sandwiches</a></li>
                                            <li><a href="index.html"></i>Combos</a></li>
                                        </ul>
                                    </li>
                                    <!-- Column 3-->
                                    <li class="menu-item-has-children mega-menu-col">
                                        <a href="index.html">Fried Chicken</a>
                                        <ul class="sub-menu">
                                            <li><a href="index.html"></i>Combos</a></li>
                                        </ul>
                                    </li>
                                </ul>
                            </div>
                        </li>
                        <!-- Mega menu end-->
                    </ul>
                </div>
            </div>
        </div>
        <!-- Shopping Cart Hamburger Menu-->
        <div class="extra-nav">
            <ul>
                <li><a class="open-offcanvas" href="#"><span>Cart</span><span class="fa fa-bars"></span></a></li>
            </ul>
        </div>
        <!-- Mobile menu-->
        <div class="nav-toogle"><a href="#" data-toggle="collapse" data-target=".inner-navigation"><span class="icon-bar"></span><span class="icon-bar"></span><span class="icon-bar"></span></a></div>
    </div>
</header>
<!-- Header end-->

<div class="wrapper">
<!-- Page Header-->
<section class="module-header full-height parallax bg-dark bg-gradient" data-background="assets/images/module-5.jpg">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <h1 class="h1">Grab It N Go</h1>
                <h1 class="h1 m-b-15">Food Mart</h1>
              <!--  <p class="m-b-30">Coming Soon</p> -->
                <p>
                    <a class="btn btn-lg btn-circle btn-white" href="#">Online Ordering Soon</a>
               <!--     <a class="btn btn-lg btn-outline btn-circle btn-white" href="#">Learn More</a> -->
                </p>
            </div>
        </div>
    </div>
</section>
<!-- Page Header end-->
</div>
    <script src="~/Scripts/jquery-2.2.4.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/tether/1.1.1/js/tether.min.js"></script>
    <script src="~/Scripts/js/bootstrap.min.js"></script>
    <script src="http://maps.googleapis.com/maps/api/js?key=AIzaSyA0rANX07hh6ASNKdBr4mZH0KZSqbHYc3Q"></script>
    <script src="~/Scripts/plugins.min.js"></script>
    <script src="~/Scripts/custom.min.js"></script>
</body>
</html>